    "@babel/preset-env": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "mongoose": "^6.0.1",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.6.0",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
